                <enumeratedValue><name>3</name><value>1</value></enumeratedValue>
                <enumeratedValue><name>2</name><value>2</value></enumeratedValue>
                <enumeratedValue><name>1</name><value>3</value></enumeratedValue>
              </enumeratedValues>
